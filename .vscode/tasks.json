{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "type": "shell",
            "command": "g++ main.cpp -g -lwinhttp -lws2_32 -o beacon-client.exe",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": "$gcc"
        },
        {
            "type": "dotnet",
            "task": "build",
            "group": "build",
            "problemMatcher": [],
            "label": "dotnet: build"
        },
        {
            "label": "Start Server",
            "type": "shell",
            "command": "dotnet run --project ${workspaceFolder}\\JmcaC2\\",
            "group": {
                "kind": "test",
                "isDefault": false
            },
            "problemMatcher": "$gcc"
        },
    ]
}